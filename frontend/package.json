{
  "name": "react-redux-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "debounce": "^1.2.0",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.7",
    "react": "^16.8.3",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.3",
    "react-icons": "^3.4.0",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^4.0.8",
    "react-select": "^2.4.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "tether": "^1.4.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js styleguide:build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage --watchAll",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "typedoc": "typedoc --out docs --mode modules --tsconfig tsconfig.json",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.12.27",
    "@types/react": "^16.8.4",
    "@types/react-autosuggest": "^9.3.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^6.0.13",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.13",
    "@types/reactstrap": "^6.4.4",
    "@types/redux-logger": "^3.0.7",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^8.0.6",
    "typedoc": "^0.13.0",
    "typescript": "^3.3.3333"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
