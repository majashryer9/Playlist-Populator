{
  "name": "react-redux-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.5",
    "react": "^16.6.3",
    "react-bootstrap-typeahead": "^3.3.0",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "tether": "^1.4.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js styleguide:build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage --watchAll",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "typedoc": "typedoc --out docs --mode modules --tsconfig tsconfig.json",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.7",
    "@types/react": "^16.7.6",
    "@types/react-bootstrap-typeahead": "^3.4.0",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^6.4.2",
    "@types/redux-logger": "^3.0.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "react-docgen-typescript": "^1.12.0",
    "react-styleguidist": "^8.0.4",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  }
}
